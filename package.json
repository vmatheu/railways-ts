{
  "name": "railways-ts",
  "version": "1.0.0",
  "description": "Library for help to railway programing",
  "main": "./lib/index.js",
  "engines": {
    "node": "14.15.4"
  },
  "scripts": {
    "mocha": "NODE_ENV=test mocha --timeout 10000 -r ts-node/register --exit",
