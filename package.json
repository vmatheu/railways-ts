    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "chai": "4.3.6",
    "eslint": "^7.32.0",
    "mocha": "^9.2.2",
    "nyc": "15.1.0",
    "prettier": "2.3.2",
    "ts-mocha": "9.0.2",
    "ts-node": "10.7.0",
    "ttypescript": "^1.5.12",
    "typescript": "4.5.5"
  },
  "files": [
